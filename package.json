    "express": "^4.17.2",
    "ipfs": "^0.62.0",
    "ipns": "^0.16.0",
    "jpeg-js": "^0.4.3",
    "js-image-generator": "^1.0.3",
    "pureimage": "^0.3.8",
    "snapsvg": "^0.5.1",
    "svg-generator": "^1.0.8",
    "text-svg": "^1.0.4"
  },
  "devDependencies": {
    "@chainlink/belt": "^0.0.4",
